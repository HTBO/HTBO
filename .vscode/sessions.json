{
  "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
  "theme": "tribe",
  "active": "default",
  "activateOnStartup": true,
  "keepExistingTerminals": false,
  "sessions": {
    "default": [
      {
        "name": "hello",
        "autoExecuteCommands": true,
        "icon": "person",
        "color": "terminal.ansiGreen",
        "commands": [
          "echo hello"
        ]
      },
      [
        {
          "name": "Install Dependencies",
          "commands": [
            "pnpm install"
          ]
        }
      ],
      [
        {
          "name": "Dev Backend",
          "commands": [
            "pnpm run dev:backend"
          ]
        },
        {
          "name": "Test Backend",
          "commands": [
            "pnpm run dev:backend:test"
          ]
        },
        {
          "name": "Backend Coverage",
          "commands": [
            "pnpx jest --coverage"
          ]
        }
      ],
      [
        {
          "name": "Dev Frontend",
          "commands": [
            "pnpm run dev:frontend"
          ]
        },
        {
          "name": "Test Frontend",
          "commands": [
            "pnpm run dev:frontend:test"
          ]
        }
      ],
      [
        {
          "name": "Parallel Dev",
          "commands": [
            "pnpm run dev"
          ]
        }
      ]
    ]
  }
}