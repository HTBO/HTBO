{
  "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
  "theme": "tribe",
  "active": "default",
  "activateOnStartup": true,
  "keepExistingTerminals": false,
  "sessions": {
    "default": [
      {
        "name": "Run APP",
        "autoExecuteCommands": true,
        "icon": "person",
        "color": "terminal.ansiGreen",
        "commands": [
          "pnpm run dev"
        ]
      },
      [
        {
          "name": "Run Backend Dev",
          "commands": [
            "pnpm run dev:backend"
          ]
        },
        {
          "name": "Run Tests",
          "commands": [
            "pnpm run dev:backend:test"
          ]
        },
        {
          "name": "Jest Coverage",
          "commands": [
            "pnpm run dev:backend:coverage"
          ]
        }
      ],
      [
        {
          "name": "Run Frontend",
          "commands": [
            "pnpm run dev:frontend"
          ]
        },
        {
          "name": "Run E2E Tests",
          "commands": [
            "pnpm run dev:frontend:test"
          ]
        }
      ]
    ]
  }
}